{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:4001": "PORT=4001 NODE_ENV=development tsx server/index.ts",
    "dev:4002": "PORT=4002 NODE_ENV=development tsx server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --target=node22 --outfile=server/dist/index.js",
    "start": "NODE_ENV=production node server/dist/index.js",
    "check": "tsc",
    "db:ping": "tsx server/scripts/db-ping.ts",
    "db:generate": "dotenv -e server/.env -- drizzle-kit generate",
    "db:push": "dotenv -e server/.env -- drizzle-kit push",
    "db:push:accept": "dotenv -e server/.env -- drizzle-kit push --force",
    "db:migrate": "tsx server/scripts/run-migrations.ts",
    "db:apply": "node apply-migration.js",
    "db:stores": "node apply-stores-only.js",
    "db:seed": "tsx server/seed.ts",
    "db:seed:subs": "tsx server/seed-substitutions.ts",
    "db:seed:subs:full": "node --import tsx server/scripts/build-and-seed-substitutions.ts",
    "db:seed:competitions": "tsx server/scripts/seed-competitions.ts",
    "db:seed:signup": "tsx server/seed-signup.ts",
    "db:export:neon": "tsx server/scripts/export-table.ts playing_with_neon",
    "postinstall": "npx update-browserslist-db@latest || true",
    "check:imports": "node tools/check-imports.js",
    "gen:shims": "node tools/generate-import-shims.js",
    "fix:filenames": "node tools/fix-filenames.js",
    "env:print": "tsx server/scripts/print-env.ts",
    "test:email": "tsx server/scripts/test-email.ts",
    "db:apply:noninteractive": "npm run db:generate && npm run db:migrate"
  },
